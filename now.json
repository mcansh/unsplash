{
  "name": "unsplash",
  "version": 2,
  "alias": [
    "unsplash.loganmcansh.com"
  ],
  "env": {
    "KEY": "@unsplash-key",
    "SECRET": "@unsplash-secret"
  },
  "builds": [
    {
      "src": "packages/www/package.json",
      "use": "@now/next"
    },
    {
      "src": "packages/api/src/index.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "packages/api/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/www/$1"
    }
  ]
}
