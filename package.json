    "@testing-library/react": "9.3.0",
    "@types/cookie": "0.3.3",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.19",
    "@types/micro-cors": "0.1.0",
    "@types/node": "12.11.1",
