{
  "name": "unsplash",
  "version": "1.0.0",
  "description": "",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "prebuild": "tsc --project tsconfig.api.json && yarn gen",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:css": "stylelint './**/*.js' './**/*.jsx' './**/*.tsx' './**/*.ts'",
    "test": "jest",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "npm-run-all --parallel lint lint:css format type-check test",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "sh scripts/ts-precommit.bash",
      "yarn lint:css",
      "git add"
    ]
  },
  "dependencies": {
    "@mcansh/custom-next-link": "0.1.0",
    "@mcansh/next-now-base-url": "1.1.0",
    "@reach/visually-hidden": "0.4.0",
    "@sentry/browser": "5.7.1",
    "@sentry/node": "5.7.1",
    "@urql/devtools": "0.1.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "apollo-datasource-rest": "0.6.6",
    "apollo-server-cache-redis": "1.1.4",
    "apollo-server-micro": "2.14.2",
    "cookie": "0.4.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.5",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.3.2",
    "next-offline": "4.0.6",
    "node-fetch": "3.1.1",
    "nookies": "2.0.8",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-is": "16.11.0",
    "react-simple-img": "2.3.7",
    "react-ssr-prepass": "1.0.7",
    "somebody": "2.0.0",
    "styled-components": "4.4.0",
    "type-fest": "0.8.1",
    "urql": "1.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.8.2",
    "@graphql-codegen/typescript": "1.8.2",
    "@graphql-codegen/typescript-operations": "1.8.2",
    "@graphql-codegen/typescript-urql": "1.8.2",
    "@mcansh/eslint-config": "2.2.4",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "9.3.0",
    "@types/cookie": "0.3.3",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.6",
    "@types/node-fetch": "2.5.2",
    "@types/reach__visually-hidden": "0.2.0",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-styled-components": "1.10.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-kentcdodds": "14.6.0",
    "eslint-config-prettier": "6.4.0",
