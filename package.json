    "next-offline": "4.0.6",
    "node-fetch": "2.6.0",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-adopt": "0.6.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.10.2",
    "somebody": "2.0.0",
    "styled-components": "4.4.0",
    "type-fest": "0.8.1",
